{
  "name": "edent",
  "version": "1.0.0",
  "description": "My bedazzling Nuxt.js project",
  "author": "",
  "private": true,
  "scripts": {
    "dev": "nodemon -r ./node_modules/dotenv/config -x babel-node -w server server",
    "build": "npm run build:server && npm run build:client",
    "start": "node -r dotenv/config .server",
    "docker:up": "docker-compose -p 'edent' up -d postgres redis",
    "docker:down": "docker-compose -p 'edent' down",
    "build:client": "env $(cat .env | xargs) nuxt build --config-file client/nuxt.config.js",
    "build:server": "babel server -d .server --delete-dir-on-start --copy-files",
    "docker:clear": "docker rm $(docker ps -a -q) -f && docker rmi $(docker images -q) -f",
    "generate": "nuxt generate",
    "test": "jest",
    "port": "kill -9 $(lsof -t -i:3000)"
  },
  "dependencies": {
    "@nuxtjs/style-resources": "^1.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "imask": "^5.2.1",
    "ioredis": "^4.14.0",
    "jsonwebtoken": "^8.5.1",
    "nuxt": "^2.10.2",
    "pg": "^7.12.1",
    "sequelize": "^5.21.0",
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/preset-env": "^7.4.3",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "install": "^0.12.2",
    "jest": "^24.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "npm": "^6.9.0",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.3"
  }
}
